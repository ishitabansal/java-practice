class Studentrecord
{
int rollno;
String name;
void insertrecord(int r, String n)                               //class for inserting values
{
rollno=r;
name=n;
}
void displayinfo()                                              //class for displaying values
{
System.out.println(rollno+" "+name);
}
}
void displayinfo()
{
System.out.println(rollno+" "+name);
}
}
class TestStudentrecord                                         //class for main method                
{
public static void main(String args[])                          //main method
{
Studentrecord s1=new Studentrecord();                           //objects of class
Studentrecord s2=new Studentrecord(); 
s1.insertrecord(105,"ishita");
s2.insertrecord(205,"rashmi");
s1.displayinfo();                             //call displayinfo function
s2.displayinfo();
}
}
