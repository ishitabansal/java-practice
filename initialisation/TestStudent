class Student_inirefrence                                //initialisation through reference
{
int id;
String name;
}
class TestStudent                                       //class containing main method
{
public static void main(String args[])                  //main method outside the class
{
Student s1=new Student();                               //objects of class
Student s2=new Student();
s1.id=124;
s1.name="Ishita";
s2.id=125;
s2.name="Anvi";
System.out.println(s1.id+" "+s1.name);
System.out.println(s2.id+" "+s2.name);
}
}
