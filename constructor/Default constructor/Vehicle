class Vehicle                         
{
int vehicle_no;
String vehicle_name;
void display()                                           //default constructor
{
System.out.println(vehicle_no+" "+vehicle_name);
}
public static void main(String args[])
{
Vehicle v1=new Vehicle();                                //obect of default constructor
Vehicle v2=new Vehicle();
v1.display();
v2.display();
}
}
