public class Continue_innerloop
{
public static void main(String[] args)
{
for(int i=1;i<=10;i++)                            //outer loop
{
for(int j=1;j<=10;j++)                            //inner loop
{
if(i==2 && j==2)                                  //if condition
{
continue;
}
System.out.println(i+ " " +j);
}
}
}
}
